<div class="container">
<h1>Semana de <%= @monday.strftime('%d %b') %> a <%= @friday.strftime('%d %b') %></h1>
</div>

<div class="container">
<table>
	<% @moods.each do |day| %>
    <tr>
      <td><%= day[0] %></td>
	  <% day[1].each do |m| %>
      	<td>| <%= image_tag("legocons/" + m.rating + ".png", alt: m.rating)  %> </td>
	  <% end %>
    </tr>
  <% end %>
</table>
</div>


<style type="text/css">

tr > td {
 padding-top: 10px;
}

</style>